<h4>USERS</h4>
<table class="table table-light table-striped table-hover table-bordered" >
  <thead class="thead-dark">
  <tr class="active">
    <th>NAME</th>
    <th>IP</th>
    <th>AGENT</th>
  </tr>
  </thead>
  <tbody>
  <% @users.each do |u| %>
  <tr  id="user_id_<%= u.id %>" class="active">
    <td><%= u.email %></td>
    <td></td>
    <td></td>
  </tr>
  <% end %>
  </tbody>
</table>

